{
  "name": "cockpit-cli",
  "version": "0.0.1",
  "description": "Parse data for cockpit using terminal options ",
  "main": "dist/cockpit-init.js",
  "bin": "npx babel-node --presets=env dist/src/cockpit-cli.js",
  "scripts": {
    "test": "eslint .",
    "build": "rm -rf dist/* && babel ./src/*.js --out-dir dist",
    "dev":
      "rm -rf dist/* && babel --watch ./src/*.js --out-dir dist --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafaelaMG/cockpit-cli.git"
  },
  "keywords": ["cokpit", "headless", "cms", "skd", "cli"],
  "author": "Rafaela Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RafaelaMG/cockpit-cli/issues"
  },
  "homepage": "https://github.com/RafaelaMG/cockpit-cli#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "cockpit-sdk": "^0.8.2",
    "execa": "^0.9.0",
    "skeleton-parser": "github:lucalanca/skeleton-parser#js-parser",
    "terminal-kit": "^1.14.3"
